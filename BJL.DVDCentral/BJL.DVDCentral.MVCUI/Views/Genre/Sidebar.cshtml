@model IEnumerable<BJL.DVDCentral.BL.Genre>


<table class="table">
    <tr>
        <th>
            View Movies By Genre:
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Description, "LoadByGenre", "Movie", new { id = item.Id }, null)
        </td>
      
    </tr>
}

    <tr>
        <td>
            @Html.ActionLink("View All", "Index", "Movie")
        </td>
    </tr>

</table>
